{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "animate.css": "^3.7.2",
    "axios": "^0.18.0",
    "bootstrap": "^4.3.1",
    "classnames": "^2.2.6",
    "history": "^4.10.1",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^2.2.0",
    "ngx-gallery": "^5.9.1",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-notifications-component": "^2.1.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.4",
    "react-text-mask": "^5.4.3",
    "save-dev": "0.0.1-security",
    "simple-react-validator": "^1.0.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "no-unused-vars": 0,
      "no-invalid-this": 0,
      "class-methods-use-this": 0,
      "consistent-this": 0,
      "no-undef": 2,
      "no-var": 2,
      "no-dupe-args": 2,
      "no-dupe-keys": 2,
      "no-duplicate-case": 2,
      "no-ex-assign": 2,
      "no-extra-boolean-cast": 2,
      "no-extra-semi": 2,
      "no-func-assign": 2,
      "no-import-assign": 0,
      "no-unreachable": 2,
      "no-unsafe-finally": 2,
      "no-unsafe-negation": 2,
      "require-atomic-updates": 0,
      "use-isnan": 2,
      "valid-typeof": 2,
      "array-callback-return": 2,
      "block-scoped-var": 2,
      "no-shadow": 2,
      "no-delete-var": 2,
      "no-shadow-restricted-names": 2,
      "no-mixed-spaces-and-tabs": 2,
      "no-class-assign": 2,
      "no-const-assign": 2,
      "no-dupe-class-members": 2,
      "no-redeclare": 2
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "create-react-component-folder": "^0.1.30",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.13.0",
    "sass-loader": "^8.0.0",
    "webpack": "^4.29.6"
  }
}
